<?xml version='1.0' encoding='utf-8'?>
<widget id="com.impulse.test.app.android" version="1.1.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <hook src="hooks/build.sh" type="before_prepare" />
    <name>Impulse Test App</name>
    <description>
        A Impulse Test App.
    </description>
    <author email="dev@localhost" href="http://localhost">
        DevSupport
    </author>
    <icon src="resources/favicon.png" />
    <content src="index.html" />
    <access origin="*" />
    <access origin="mailto:*" launch-external="yes"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-compat" spec="~1.1.0" />
    <plugin name="cordova-plugin-camera" spec="~2.4.0" />
</widget>
